{
    "name": "dachcom-digital/emailizr",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore Emailizr - create html emails the right way!",
    "keywords": ["pimcore", "email", "inky", "inline style"],
    "homepage": "https://github.com/dachcom-digital/pimcore-emailizr",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmailizrBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "EmailizrBundle\\EmailizrBundle"
            ]
        }
    },
    "require": {
        "pimcore/pimcore": "^11.0",
        "twig/twig": "^3.9",
        "twig/inky-extra": "^3.0",
        "pelago/emogrifier": "^7.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-symfony": "^3.1",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "symplify/easy-coding-standard": "^9.0"
    }
}
